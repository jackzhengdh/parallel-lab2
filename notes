Code: 

	double tstart = 0.0, ttaken;
	tstart = omp_get_wtime();

	#pragma omp parallel for num_threads(thread_count)
	for (i = 2; i < N; i++) {
		if (isPrime[i]) {
			int j;
			for (j = i + i; j < N; j += i)
				isPrime[j] = 0;
		}
	}

	int cnt = 0;
	for (i = 2; i < N; i++) {
		if (isPrime[i]) {
			primes[nPrime++] = i;
			cnt++;
		}
	}
	ttaken = omp_get_wtime() - tstart;

